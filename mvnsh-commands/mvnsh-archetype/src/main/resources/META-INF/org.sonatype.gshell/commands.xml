<commands xmlns="http://sonatype.org/gshell-commands/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://sonatype.org/gshell-commands/1.0.0 http://sonatype.org/xsd/gshell-commands-1.0.0.xsd"
          version="1.0.0">

    <command-set id="archetype">
        <enabled>true</enabled>
        <autoRegisterPriority>60</autoRegisterPriority>
        <command>
            <action>org.sonatype.maven.archetype.commands.RealizeCommand</action>
        </command>
        <command>
            <action>org.sonatype.maven.archetype.commands.CreateCommand</action>
        </command>
        <command>
            <action>org.sonatype.maven.archetype.commands.CrawlCommand</action>
        </command>
    </command-set>

</commands>

